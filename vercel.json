{
    "version": 2,
    "name": "miblog-backend",
    "buildCommand": "npm start",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}